{
  "name": "WebScrape",
  "version": "1.0.0",
  "short_name": "WebScraper",
  "description": "An extension to helps with webscraping",
  "manifest_version": 2,
  "permissions": ["*://*", "storage"],
    "content_scripts": [
    {
        "matches": [ "*://www.messenger.com/*"],
        "run_at": "document_end",
        "js": ["index.js"],
        "all_frames": true
    }
  ],
  "web_accessible_resources": ["index.js"]
}
